package main

import (
	"fmt"
	"sort"
)

type StringSlice struct {
	s []string
}

func (s *StringSlice) Len() int {
	return len(s.s)
}

func (s *StringSlice) Less(i, j int) bool {
	return s.s[i] < s.s[j]
}

func (s *StringSlice) Swap(i, j int) {
	temp := s.s[i]
	s.s[i] = s.s[j]
	s.s[j] = temp
}

func main() {
	ss := StringSlice{
		s: []string{"Hari", "Prathap", "Balaguru"},
	}
	sort.Sort(&ss)
	fmt.Println(ss.s)
}
