package main

import (
	"fmt"
	"io/ioutil"
	"log"
	"net/http"
	"strings"
)

func main() {
	url := "www.york.ac.uk/teaching/cws/wws/webpage1.html"

	if !strings.HasPrefix(url, "https://") {
		url = strings.Join([]string{"https://", url}, "")
	}

	resp, r_err := http.Get(url)
	if r_err != nil {
		log.Fatalln(r_err)
	}

	defer resp.Body.Close()
	res, readErr := ioutil.ReadAll(resp.Body)
	if readErr != nil {
		log.Println(readErr)
	}
	fmt.Printf("%s", res)
}
